{
  "name": "angular-react",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:demo": "ng build --app demo",
    "build:demo:prod": "ng build --app demo --prod",
    "build:demo:prod:package": "ng build --app demo-from-packages --prod",
    "build:core": "ng-packagr -p libs/core/package.json",
    "build:fabric": "ng-packagr -p libs/fabric/package.json",
    "build:material": "ng-packagr -p libs/material/package.json",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update check",
    "update:skip": "./node_modules/.bin/nx update skip",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "publish:core": "npm run build:core && npm adduser && npm publish ./@angular-react/core.tgz --access=public",
    "publish:fabric": "npm run build:fabric && npm adduser && npm publish ./@angular-react/fabric.tgz --access=public",
    "publish:material": "npm run build:material && npm adduser && npm publish ./@angular-react/material.tgz --access=public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "5.2.7",
    "@angular/common": "5.2.7",
    "@angular/compiler": "5.2.7",
    "@angular/core": "5.2.7",
    "@angular/forms": "5.2.7",
    "@angular/platform-browser": "5.2.7",
    "@angular/platform-browser-dynamic": "5.2.7",
    "@angular/router": "5.2.7",
    "@ngrx/effects": "5.2.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "@nrwl/nx": "0.10.0",
    "core-js": "^2.4.1",
    "ngrx-store-freeze": "^0.2.1",
    "office-ui-fabric-react": "^5.78.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.7",
    "@angular/language-service": "5.2.7",
    "@ngrx/schematics": "5.2.0",
    "@nrwl/schematics": "0.10.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/react": "^16.3.4",
    "@types/react-dom": "^16.0.4",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.1",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
